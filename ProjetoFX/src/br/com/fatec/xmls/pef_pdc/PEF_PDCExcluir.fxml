<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="recicla.telas.PEF_PDCController"
      spacing="20" alignment="TOP_CENTER" stylesheets="/recicla/css/style.css">
    <children>
        <Label text="Excluir Coletas" styleClass="title-label" />
        <TableView fx:id="table" prefHeight="300.0" />
        <HBox alignment="CENTER" spacing="10">
            <Button fx:id="btnExcluir" text="Excluir" />
            <Button fx:id="btnCancelar" text="Cancelar" />
        </HBox>
    </children>
</VBox>
